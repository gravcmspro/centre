{% if values.activator_label is not null %}
<a class="modal-trigger waves-effect waves-light btn" href="#{{ values.id }}">{{ values.activator_label }}</a>
{% endif %}

<div id="{{ values.id }}" class="modal">
    <div class="modal-content">
      {{ values.content }}
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat ">{{ values.closer_label }}</a>
    </div>
</div>
